#: /usr/lib/python3.4/argparse.py:300
msgid "usage: "
msgstr "Gebrauch: "

#: /usr/lib/python3.4/argparse.py:831
msgid ".__call__() not defined"
msgstr ".__call__() nicht definiert"

#: /usr/lib/python3.4/argparse.py:1128
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "Unbekannter Parser %(parser_name)r (Wahlmöglichkeiten: %(choices)s)"

#: /usr/lib/python3.4/argparse.py:1182
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "Argument \"-\" im Modus %r"

#: /usr/lib/python3.4/argparse.py:1190
#, python-format
msgid "can't open '%s': %s"
msgstr "'%s' kann nicht geöffnet werden: %s"

#: /usr/lib/python3.4/argparse.py:1399
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "Kann Aktionen nicht zusammenführen: Zwei Gruppen heißen %r."

#: /usr/lib/python3.4/argparse.py:1437
msgid "'required' is an invalid argument for positionals"
msgstr "'required' ist ein ungültiges Argument für lageabhängige Argumente."

#: /usr/lib/python3.4/argparse.py:1459
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"Ungültige Optionszeichenkette %(option)r: Muss mit einem Buchstaben "
"%(prefix_chars)s beginnen."

#: /usr/lib/python3.4/argparse.py:1479
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= ist erforderlich für Optionen wie %r."

#: /usr/lib/python3.4/argparse.py:1496
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "Ungültiger 'conflict_resolution'-Wert: %r"

#: /usr/lib/python3.4/argparse.py:1514
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "Widersprüchliche Optionszeichenkette: %s"
msgstr[1] "Widersprüchliche Optionszeichenketten: %s"

#: /usr/lib/python3.4/argparse.py:1580
msgid "mutually exclusive arguments must be optional"
msgstr "Sich gegenseitig auschließende Argumente müssen optional sein."

#: /usr/lib/python3.4/argparse.py:1640
msgid "positional arguments"
msgstr "Lageabhängige Argumente"

#: /usr/lib/python3.4/argparse.py:1641
msgid "optional arguments"
msgstr "Optionale Argumente"

#: /usr/lib/python3.4/argparse.py:1656
msgid "show this help message and exit"
msgstr "Zeige diesen Hilfetext und höre auf."

#: /usr/lib/python3.4/argparse.py:1687
msgid "cannot have multiple subparser arguments"
msgstr "Kann nicht mehrere Subparserargumente haben."

#: /usr/lib/python3.4/argparse.py:1739
#, python-format
msgid "unrecognized arguments: %s"
msgstr "Unerkanntes Argument: %s"

#: /usr/lib/python3.4/argparse.py:1836
#, python-format
msgid "not allowed with argument %s"
msgstr "nicht erlaubt mit dem Argument %s"

#: /usr/lib/python3.4/argparse.py:1882 /usr/lib/python3.4/argparse.py:1896
#, python-format
msgid "ignored explicit argument %r"
msgstr "explizites Argument %s ignoriert"

#: /usr/lib/python3.4/argparse.py:2003
#, python-format
msgid "the following arguments are required: %s"
msgstr "Die folgenden argumente sind erforderlich: %s."

#: /usr/lib/python3.4/argparse.py:2018
#, python-format
msgid "one of the arguments %s is required"
msgstr "Eins der Argumente %s ist erforderlich."

#: /usr/lib/python3.4/argparse.py:2061
msgid "expected one argument"
msgstr "Ein Argument erwartet"

#: /usr/lib/python3.4/argparse.py:2062
msgid "expected at most one argument"
msgstr "Höchstens ein Argument erwartet"

#: /usr/lib/python3.4/argparse.py:2063
msgid "expected at least one argument"
msgstr "Mindestens ein Argument erwartet"

#: /usr/lib/python3.4/argparse.py:2065
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "%s Argument erwartet"
msgstr[1] "%s Argumente erwartet"

#: /usr/lib/python3.4/argparse.py:2124
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "Mehrdeutige Option: %(option)s könnte auf %(matches)s passen."

#: /usr/lib/python3.4/argparse.py:2187
#, python-format
msgid "unexpected option string: %s"
msgstr "Unerwartete Optionszeichenkette: %s"

#: /usr/lib/python3.4/argparse.py:2291
#, python-format
msgid "%r is not callable"
msgstr "%s ist nicht aufrufbar."

#: /usr/lib/python3.4/argparse.py:2308
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "Ungültiger %(type)s-Wert: %(value)r."

#: /usr/lib/python3.4/argparse.py:2319
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "Ungültige Auswahl: %(value)r (wähle aus %(choices)s)."

#: /usr/lib/python3.4/argparse.py:2395
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: Fehler: %(message)s\n"

