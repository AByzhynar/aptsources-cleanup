#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: Fehler: %(message)s\n"

#, python-format
msgid "%r is not callable"
msgstr "%s ist nicht aufrufbar."

msgid "'required' is an invalid argument for positionals"
msgstr "'required' ist ein ungültiges Argument für lageabhängige Argumente."

#, python-brace-format
msgid "'{file:s}' contains no valid and enabled repository lines.  Do you want to remove it?"
msgstr "'{file:s}' enthält keine gültigen aktiven Verzeichniszeilen.  Wollen Sie sie entfernen?"

#, python-brace-format
msgid "'{path:s}' removed."
msgstr "'{path:s}' entfernt."

msgid ".__call__() not defined"
msgstr ".__call__() nicht definiert"

msgid "Aborted."
msgstr "Abgebrochen."

msgid "Apply all changes without question."
msgstr "Wende alle Änderungen ohne Nachfrage an."

msgid "Author"
msgstr "Autor"

msgid "Cannot check package integrity"
msgstr "Kann Paketintegrität nicht üperprüfen"

msgid "Cannot wrap text output due a failure to get the terminal size"
msgstr "Kann den Ausgabetext nicht umbrechen wegen eines Fehlers beim Bezug der Terminalgröße"

msgid "Debugging Options"
msgstr "Debugging-Optionen"

#, python-brace-format
msgid "Do you have the '{package:s}' package installed?  You can do so with:"
msgstr "Haben Sie das Paket '{package:s} installiert? Sie können das nachholen mit:"

msgid "Do you want to save these changes?"
msgstr "Wollen Sie diese Änderungen sichern?"

msgid "Error"
msgstr "Fehler"

msgid "For wizards only! Use these if you know and want to test the application source code."
msgstr "Nur für Hexenmeister! Benuzte sie, wenn du den Anwendungsquelltext kennst und ihn testen möchtest."

#, python-brace-format
msgid "Force an ImportError for the '{module:s}' module and fail on all subsequent diagnoses."
msgstr "Erzwinge einen ImportError für das '{module:s}'-Modul und lass alle anschließenden Diagnosen fehlschlagen."

# Detects and interactively deactivates duplicate Apt source entries and deletes sources list files without valid enabled source entries in '/etc/sources.list' and '/etc/sources.list.d/*.list'.
msgid "ID_DESCRIPTION"
msgstr "Findet und deaktiviert doppelte Apt-Quelleneinträge und löscht Quellenlistendateien ohne gültige aktive Quelleneinträge in '/etc/sources.list' und '/etc/sources.list.d/*.list' interaktiv."

#, python-brace-format
msgid "Load sources list files from this directory instead of the default root-owned '{default:s}'. If omitted DIR defaults to '{const:s}'."
msgstr "Lade Quellenlistendateien von diesem Verzeichnis anstelle der Vorgabe in root-Besitz '{default:s}'. Falls ausgelassen ist das Verzeichnis '{const:s}'."

msgid "Never apply changes; only print what would be done."
msgstr "Wende Änderungen nie an; zeige lediglich an, was getan würde."

msgid "No duplicate entries were found."
msgstr "Keine doppelten Einträge gefunden."

msgid "No such directory"
msgstr "Kein solches Verzeichnis"

#, python-brace-format
msgid ""
"Overlapping source entries:\n"
"  1. file {orig_file:s}:\n"
"     {orig_line:s}\n"
"  2. file {dupe_file:s}:\n"
"     {dupe_line:s}\n"
"I disabled the latter entry."
msgstr ""
"Überlappende Quelleneinträge:\n"
"  1. Datei {orig_file:s}:\n"
"     {orig_line:s}\n"
"  2. Datei {dupe_file:s}:\n"
"     {dupe_line:s}\n"
"Ich habe letzteren Eintrag deaktiviert."

msgid "Package integrity check failed"
msgstr "Paketintegritätsprüfung schlug fehl"

#, python-brace-format
msgid "Please make sure that the '{package:s}' package wasn't corrupted and that '{py_exe:s}' refers to the Python interpreter from the same package."
msgstr "Bitte stellen Sie sicher, dass das Paket '{package:s}' nicht beschädigt wurde und dass '{py_exe:s}' sich auf den Python-Interpreter aus dem selben Paket bezieht."

msgid "Show help for debugging options."
msgstr "Zeige die Hilfe für Debugging-Optionen an."

msgid "Source code and bug tracker location"
msgstr "Quelltext- und Bug-Tracker-Adresse"

#, python-brace-format
msgid "The current Python interpreter is '{py_exe:s}'.  Please use the default '{py_exe_default:s}' if you encounter issues with the import of the '{module:s}' module."
msgstr "Der derzeitige Python-Interpretet ist '{py_exe:s}'.  Bitte benutzen sie Vorgabe '{py_exe_default:s}', falls Sie Problemen beim Import des Moduls '{module:s}' begegnen."

msgid "Warning"
msgstr "Warnung"

msgid "all"
msgstr "alle"

#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "Mehrdeutige Option: %(option)s könnte auf %(matches)s passen."

#, python-format
msgid "argument \"-\" with mode %r"
msgstr "Argument \"-\" im Modus %r"

#, python-format
msgid "can't open '%s': %s"
msgstr "'%s' kann nicht geöffnet werden: %s"

msgid "cannot have multiple subparser arguments"
msgstr "Kann nicht mehrere Subparserargumente haben."

#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "Kann Aktionen nicht zusammenführen: Zwei Gruppen heißen %r."

#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "Widersprüchliche Optionszeichenkette: %s"
msgstr[1] "Widersprüchliche Optionszeichenketten: %s"

#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= ist erforderlich für Optionen wie %r."

msgid "display"
msgstr "anzeigen"

msgid "empty"
msgstr "leer"

#, python-brace-format
msgid "exit status {status:d}"
msgstr "Ausstiegstatus {status:d}"

#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "%s Argument erwartet"
msgstr[1] "%s Argumente erwartet"

msgid "expected at least one argument"
msgstr "Mindestens ein Argument erwartet"

msgid "expected at most one argument"
msgstr "Höchstens ein Argument erwartet"

msgid "expected one argument"
msgstr "Ein Argument erwartet"

#, python-format
msgid "ignored explicit argument %r"
msgstr "explizites Argument %s ignoriert"

#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "Ungültiger %(type)s-Wert: %(value)r."

#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "Ungültige Auswahl: %(value)r (wähle aus %(choices)s)."

#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "Ungültiger 'conflict_resolution'-Wert: %r"

#, python-format
msgid "invalid option string %(option)r: must start with a character %(prefix_chars)r"
msgstr "Ungültige Optionszeichenkette %(option)r: Muss mit einem Buchstaben %(prefix_chars)s beginnen."

msgid "mutually exclusive arguments must be optional"
msgstr "Sich gegenseitig auschließende Argumente müssen optional sein."

msgid "no"
msgstr "nein"

msgid "none"
msgstr "keine"

#, python-format
msgid "not allowed with argument %s"
msgstr "nicht erlaubt mit dem Argument %s"

#, python-format
msgid "one of the arguments %s is required"
msgstr "Eins der Argumente %s ist erforderlich."

msgid "optional arguments"
msgstr "Optionale Argumente"

msgid "positional arguments"
msgstr "Lageabhängige Argumente"

msgid "show this help message and exit"
msgstr "Zeige diesen Hilfetext und höre auf."

#, python-format
msgid "the following arguments are required: %s"
msgstr "Die folgenden argumente sind erforderlich: %s."

#, python-format
msgid "unexpected option string: %s"
msgstr "Unerwartete Optionszeichenkette: %s"

#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "Unbekannter Parser %(parser_name)r (Wahlmöglichkeiten: %(choices)s)"

#, python-format
msgid "unrecognized arguments: %s"
msgstr "Unerkanntes Argument: %s"

msgid "usage: "
msgstr "Gebrauch: "

msgid "yes"
msgstr "ja"

#, python-brace-format
msgid "{nduplicates:d} source entry was disabled"
msgid_plural "{nduplicates:d} source entries were disabled"
msgstr[0] "{nduplicates:d} Quelleneintrag wurde deaktiviert"
msgstr[1] "{nduplicates:d} Quelleneinträge wurden deaktiviert"

#, python-brace-format
msgid "{nremoved:d} of {ntotal:d} empty sourcelist files removed."
msgstr "{nremoved:d} von {ntotal:d} Quellenlistendateien entfernt."
